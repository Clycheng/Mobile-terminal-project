rem(px)
  return px/14rem
html
    font-size 14px
    background #b8b8b8
.navbar-inverse
  background #169f85
  border-color white
  color white
.navbar-inverse .navbar-toggle
  border-color white
.navbar-inverse .navbar-toggle:hover
  background none
.navbar-inverse .navbar-brand
  color white
.navbar-inverse .navbar-nav > li > a
  color white
  float left
  padding 5% 8%
.navbar-inverse .navbar-collapse, .navbar-inverse .navbar-form
  border-color white
section#list-warp
  width 100%
  height 100%
  position relative
  top rem(50)
.list-content
  width 100%
  height 100%
  overflow hidden
.list-content ul
  list-style none
  box-sizing border-box
  -webkit-padding-start 0
.list-content ul li
  margin-bottom 2%
  width 100%
  height 100%
  list-style  none
  background white
  box-sizing border-box
.list-content ul li .return-detail
  display block
  width 100%
  height rem(110)
  padding rem(2)
.list-content ul li .return-detail .left-warp
  float left
  width 30%
  height 100%
  box-sizing border-box
.list-content ul li .return-detail .left-warp img
  width 100%
  height 100%
.list-content ul li .return-detail .middle-warp
  float left
  width 50%
  height 100%
  padding 0 rem(5)
  box-sizing border-box
.list-content ul li .return-detail .middle-warp h3
  display -webkit-box
  margin-top 0
  margin-bottom 0
  width 100%
  height 50%
  font-size rem(14)
  color #1d1d1d
  text-overflow ellipsis
  word-wrap:break-word
  -webkit-line-clamp 4
  -webkit-box-orient vertical
  box-sizing border-box
.list-content ul li .return-detail .middle-warp h3:hover
  color red
  text-decoration underline
.list-content ul li .return-detail .middle-warp p
  width 100%
  height 50%
  line-height rem(52)
  color #9d9d9d
  overflow hidden
  text-overflow: ellipsis
  white-space: nowrap
  box-sizing border-box
.list-content ul li .return-detail .right-warp
  float left
  width 20%
  height 100%
  box-sizing border-box
.list-content ul li .return-detail .right-warp b
  display block
  width 100%
  height 50%
  line-height rem(52)
  text-align center
  box-sizing border-box
.list-content ul li .return-detail .right-warp .join-cart
  display block
  width 100%
  height 50%
  line-height rem(52)
  text-align center
  box-sizing border-box
.list-content ul li .return-detail .right-warp .join-cart img
  width 50%
  height 50%



